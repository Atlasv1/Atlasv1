   local text   = ""
local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
local function say() 
Event:FireServer(text, "All")
end
text = "gg.777-Hub[made by gg.lui]" say()
end)

